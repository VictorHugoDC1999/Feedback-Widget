//criando tabela de banco de dados, comando:npx prisma migrate dev 

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Feedback {
  id String @id @default(uuid())

  type       String
  comment    String
  screenshot String?


  @@map("feedbacks")
}